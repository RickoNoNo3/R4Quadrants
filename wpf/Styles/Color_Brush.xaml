<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="BoxColor0" R="0x77" G="0x44" B="0x44" A="0xFF" />
  <Color x:Key="BoxColor1" R="0xAA" G="0xAA" B="0x44" A="0xFF" />
  <Color x:Key="BoxColor2" R="0x44" G="0x44" B="0x44" A="0xFF" />
  <Color x:Key="BoxColor3" R="0x44" G="0x33" B="0x77" A="0xFF" />
  <SolidColorBrush x:Key="BoxBrush0" Color="{StaticResource BoxColor0}" />
  <SolidColorBrush x:Key="BoxBrush1" Color="{StaticResource BoxColor1}" />
  <SolidColorBrush x:Key="BoxBrush2" Color="{StaticResource BoxColor2}" />
  <SolidColorBrush x:Key="BoxBrush3" Color="{StaticResource BoxColor3}" />
</ResourceDictionary>